{
    "ID": "anonymous",
    "Type": "client",
    "Rules": "{{"
key_prefix \"\" {
    policy = \"deny\"
}

key_prefix \"oauth2-clients\" {
    policy = \"read\"
}

key_prefix \"concourse/workers/sshpub\" {
    policy = \"read\"
}

node \"\" {
    policy = \"read\"
}

service_prefix \"\" {
    policy = \"read\"
}

